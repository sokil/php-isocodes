# 1977年ジブチ共和国として独立
# 旧仏領
#. Name for AIDJ
msgid "French Afars and Issas"
msgstr "仏領アファール及びイッサ"

#. Name for ANHH
msgid "Netherlands Antilles"
msgstr "オランダ領アンティル諸島"

#. Name for BQAQ
msgid "British Antarctic Territory"
msgstr "英国南極領土"

#. Name for BUMM
msgid "Burma, Socialist Republic of the Union of"
msgstr "ビルマ社会主義連邦共和国"

#. Name for BYAA
msgid "Byelorussian SSR Soviet Socialist Republic"
msgstr "白ロシア SSR ソビエト社会主義共和国"

#. Name for CSHH
msgid "Czechoslovakia, Czechoslovak Socialist Republic"
msgstr "チェコスロヴァキア社会主義共和国"

#. Name for CSXX
msgid "Serbia and Montenegro"
msgstr "セルビア及びモンテネグロ"

# 現キリバス共和国領
# 過去は英領・米領
#. Name for CTKI
msgid "Canton and Enderbury Islands"
msgstr "カントン及びエンダーベリ島"

# 旧東ドイツ
# 1990年に西ドイツに吸収される
#. Name for DDDE
msgid "German Democratic Republic"
msgstr "ドイツ民主共和国"

# ベナンの旧称
# ダホメーと記述されることもある
#. Name for DYBJ
msgid "Dahomey"
msgstr "ダオメー"

#. Name for FQHH
msgid "French Southern and Antarctic Territories"
msgstr "フランス南方および南極領土"

#. Name for FXFR
msgid "France, Metropolitan"
msgstr "フランス, メトロポリタン"

# 旧英領
# ギルバート諸島は 1979 年にキリバスとして独立
# エリス諸島は 1978 年にツバルとして独立
#. Name for GEHH
msgid "Gilbert and Ellice Islands"
msgstr "ギルバート及びエリス諸島"

# ブルキナ・ファソの旧称
# 上ボルタとも呼ばれる
#. Name for HVBF
msgid "Upper Volta, Republic of"
msgstr "アッパーボルタ共和国"

# ハワイ西1130km 米領ジョンストン島 (環礁)
# 現在は UMI (アメリカ合衆国外諸島) として扱われる
#. Name for JTUM
msgid "Johnston Island"
msgstr "ジョンストン島"

# 北太平洋にある環礁
# 米領
#. Name for MIUM
msgid "Midway Islands"
msgstr "ミッドウェー諸島"

# 旧英仏信託統治領
# 現在はバヌアツ共和国の一部を構成する諸島になっている
#. Name for NHVU
msgid "New Hebrides"
msgstr "ニューヘブリディーズ"

# 南極大陸で過去にノルウェーが領土と主張していた範囲
# 南極条約により主張は凍結されている
# クイーン・モード・ランドとも記述される
#. Name for NQAQ
msgid "Dronning Maud Land"
msgstr "ドローニングモードランド"

#. Name for NTHH
msgid "Neutral Zone"
msgstr "中立地帯"

# check from here
#. Name for PCHH
msgid "Pacific Islands (trust territory)"
msgstr "太平洋諸島 (信託統治領)"

#. Name for PUUM
msgid "US Miscellaneous Pacific Islands"
msgstr "米領太平洋諸島"

#. Name for PZPA
msgid "Panama Canal Zone"
msgstr "パナマ運河地区"

# ジンバブエの植民地時代の呼称
#. Name for RHZW
msgid "Southern Rhodesia"
msgstr "南ローデシア"

# 現在はインドのシッキム州となっている
# 1975年にインドに編入されて王政が廃止
#. Name for SKIN
msgid "Sikkim"
msgstr "シッキム"

#. Name for SUHH
msgid "USSR, Union of Soviet Socialist Republics"
msgstr "USSR ソビエト社会主義共和国連邦"

#. Name for TPTL
msgid "East Timor"
msgstr "東ティモール"

#. Name for VDVN
msgid "Viet-Nam, Democratic Republic of"
msgstr "ベトナム民主共和国"

# 米領ウェーク島 (環礁)
# UMI に統合
#. Name for WKUM
msgid "Wake Island"
msgstr "ウェーク島"

#. Name for YDYE
msgid "Yemen, Democratic, People's Democratic Republic of"
msgstr "イエメン民主主義人民共和国"

#. Name for YUCS
msgid "Yugoslavia, Socialist Federal Republic of"
msgstr "ユーゴスラビア社会主義連邦共和国"

# コンゴ民主共和国の旧称
#. Name for ZRCD
msgid "Zaire, Republic of"
msgstr "ザイール共和国"
